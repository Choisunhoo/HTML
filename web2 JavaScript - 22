    <h1>Loop & Array</h1>
    <script>
      var menu = ['chicken', 'pizza', 'buger', 'pasta', 'kimchi']
    </script>
    <h2>Menu</h2>
    <ul>
      <script>
      var i = 0;
      while(i < menu.length){
        document.write('<li>'+menu[i]+'</li>');
        i += 1
      }
      </script>
  </body>
